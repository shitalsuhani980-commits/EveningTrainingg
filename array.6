class array6
{
    public static void main(String args[])
	{
	    int arr[]={10,5,15,20,6};
		System.out.println("original array");
		for(int i=0;i<arr.length-1;i++)
		{
		    System.out.print(arr[i]);
		}
		System.out.println("reversed array");
		fot(int i=arr.length-1;i>=0;i--)
		{
		    System.out.print(arr[i]+" ");
		}
	}
}